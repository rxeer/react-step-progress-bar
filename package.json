  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react-transition-group": "^4.4.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o .",
